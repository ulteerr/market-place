{
  "name": "marketplace-fronted",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nuxt dev --host 0.0.0.0 --port 3000",
    "build": "nuxt build",
    "preview": "nuxt preview --host 0.0.0.0 --port 3000",
    "prepare": "nuxt prepare",
    "test": "playwright test --project=chrome --project=mozilla-firefox --project=safari-webkit tests/e2e/admin-auth.spec.ts",
    "pretest:e2e": "playwright install --with-deps chrome firefox webkit",
    "test:e2e": "playwright test",
    "pretest:e2e:ui": "playwright install --with-deps chrome firefox webkit",
    "test:e2e:ui": "playwright test --ui",
    "pretest:e2e:headed": "playwright install --with-deps chrome firefox webkit",
    "test:e2e:headed": "playwright test --headed",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "nuxt": "^4.0.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^25.2.1",
    "prettier": "^3.6.2",
    "sass": "^1.83.4",
    "tailwindcss": "^4.1.18"
  }
}
