{
  "name": "marketplace-frontend",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nuxt dev --host 0.0.0.0 --port 3000",
    "build": "nuxt build",
    "preview": "nuxt preview --host 0.0.0.0 --port 3000",
    "prepare": "nuxt prepare",
    "storybook": "storybook dev -p 6006 --host 0.0.0.0 --ci",
    "build-storybook": "storybook build",
    "pretest": "playwright install --with-deps chromium firefox webkit",
    "test": "playwright test --project=chrome --project=mozilla-firefox --project=safari-webkit tests/e2e",
    "pretest:e2e": "playwright install --with-deps chromium firefox webkit",
    "test:e2e": "playwright test",
    "pretest:e2e:ui": "playwright install --with-deps chromium firefox webkit",
    "test:e2e:ui": "playwright test --ui",
    "pretest:e2e:headed": "playwright install --with-deps chromium firefox webkit",
    "test:e2e:headed": "playwright test --headed",
    "test:unit": "vitest run",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "nuxt": "^4.0.3"
  },
  "devDependencies": {
    "@codemirror/commands": "^6.9.1",
    "@codemirror/lang-css": "^6.3.1",
    "@codemirror/lang-html": "^6.4.11",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/lang-json": "^6.0.2",
    "@codemirror/lang-sql": "^6.10.0",
    "@codemirror/language": "^6.11.3",
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.39.10",
    "@nuxtjs/i18n": "^10.1.2",
    "@playwright/test": "^1.55.0",
    "@storybook/addon-a11y": "9.1.13",
    "@storybook/addon-docs": "9.1.13",
    "@storybook/addon-links": "9.1.13",
    "@storybook/vue3-vite": "9.1.13",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^25.2.1",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/test-utils": "^2.4.6",
    "jsdom": "^28.1.0",
    "prettier": "^3.6.2",
    "sass": "^1.83.4",
    "storybook": "9.1.13",
    "tailwindcss": "^4.1.18",
    "unplugin-auto-import": "^21.0.0",
    "vitest": "^3.2.4"
  }
}
